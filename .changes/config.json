{
  "gitSiteUrl": "https://github.com/zakuciael/rofi-jetbrains/",
  "pkgManagers": {
    "rust": {
      "version": true,
      "publish": "echo Publishing version ${ pkgFile.version } to GitHub releases",
      "getPublishedVersion": "cargo metadata -q --offline --no-deps --format-version 1 | jq -r '.packages[0].version'"
    }
  },
  "packages": {
    "rofi-jetbrains": {
      "path": ".",
      "manager": "rust",
      "assets": [
        {
          "path": "./target/release/librofi_jetbrains.so",
          "name": "jetbrains.so"
        }
      ]
    }
  }
}